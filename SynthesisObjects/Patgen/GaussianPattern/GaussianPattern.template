    };
    PARAMETER_RANGES = {
        CenterX = {
                INSPECTOR = SIMNumberInspector;
                SELECTOR = intNumber;
                RANGE = (0.0,50.0);
                FORMAT = "%.2g";
        };
        CenterY = {
                INSPECTOR = SIMNumberInspector;
                SELECTOR = intNumber;
                RANGE = (0.0,50.0);
                FORMAT = "%.2g";
        };
        Radius = {
                INSPECTOR = SIMNumberInspector;
                SELECTOR = intNumber;
                RANGE = (0.0,50.0);
                FORMAT = "%.2g";
        };
        Constant = {
                INSPECTOR = SIMNumberInspector;
                SELECTOR = floatNumber;
                RANGE = (0.0,1.0);
                FORMAT = "%.2g";
        };
        Peak = {
                INSPECTOR = SIMNumberInspector;
                SELECTOR = floatNumber;
                RANGE = (0.0,1000.0);
                FORMAT = "%.2g";
        };
        Width = {
                INSPECTOR = SIMNumberInspector;
                SELECTOR = floatNumber;
                RANGE = (0.0,50.0);
                FORMAT = "%.2g";
